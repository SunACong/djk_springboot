<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szj.djk.mapper.AbnormalProcessMapper">

    <resultMap type="com.szj.djk.entity.AbnormalProcess" id="BaseResultMap">
        <result property="id"    column="id"    />
        <result property="number"    column="number"    />
        <result property="productNum"    column="product_num"    />
        <result property="nowProduce"    column="now_produce"    />
        <result property="stopTime"    column="stop_time"    />
        <result property="note"    column="note"    />
    </resultMap>

    <sql id="selectAbnormalProcessVo">
        select id, number, product_num, now_produce, stop_time, note from abnormal_process
    </sql>


</mapper>
